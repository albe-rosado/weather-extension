{
  "manifest_version": 2,

  "name": "Weather Data",
  "description": "This extension alerts in case of storm or any other weather anomaly",
  "version": "1.0",

  "browser_action": {
    "default_icon": "thunder.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://api.openweathermap.org/",
    "geolocation"
]
}